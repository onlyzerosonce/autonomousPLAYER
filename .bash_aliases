alias debug='PS4='\''Line ${LINENO}: '\'' bash -x ' 
# alias m='bash m.sh'
alias m='exec terminator -p Transparent --geometry 800x600+5+25 -x /bin/bash $HOME/autonomousPLAYER/m.sh & '
alias del='bash autonomousPLAYER/delmu.sh manual'
alias i='bash autonomousPLAYER/interact.sh'
alias killplayer='echo killing player && rm .autonaumasplayerinput'
alias startplayer='echo Starting player && touch .autonaumasplayerinput&&bash autonomousPLAYER/autonaumusPLAYER.sh&'
alias m2='mplayer -softvol -softvol-max 300 '
alias vpnconnect='/opt/cisco/anyconnect/bin/vpn -s < vpnconnectdata.dat'
alias vpndisconnect='/opt/cisco/anyconnect/bin/vpn disconnect'
alias vpnconnectui='/opt/cisco/anyconnect/bin/vpnui'
alias charging='upower -i /org/freedesktop/UPower/devices/battery_BAT0|head -20|egrep -io "discharging"||echo it is charging'
alias st='find /3/00000000/songs/|grep -i '\''sleep trance'\''>>.delete'
alias shutdownat='touch /tmp/lock-shutdowninprogress;shutdown -h '  
alias shutdownatc='rm /tmp/lock-shutdowninprogress;shutdown -c '
alias keepplaying='echo Starting && touch .keepplayinginput && { nohup bash autonomousPLAYER/keepplaying.sh & }  && echo Started'
alias stopplaying='echo Stoping && rm .keepplayinginput && echo stopped'
alias settings='echo Running gnome-control-center && sleep 1s &&gnome-control-center'
alias spd='./anaconda3/bin/spyder'
